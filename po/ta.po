#: ../data/gnome-dictionary-source.ui.h:1
msgid "Dictionaries"
msgstr "அகராதிகள்"

#: ../data/gnome-dictionary-source.ui.h:2
msgid "H_ostname:"
msgstr "புரவலன் பெயர்: (_o)"

